{
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "start": "ts-node-dev src/server.ts",
    "typeorm": "ts-node-dev node_modules/.bin/typeorm",
    "migration:create": "ts-node-dev node_modules/.bin/typeorm migration:create src/database/migrations/$npm_config_name",
    "migration:run": "ts-node-dev node_modules/.bin/typeorm migration:run -d database.ts",
    "migration:revert": "ts-node-dev node_modules/.bin/typeorm migration:revert -d database.ts"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/pg": "^8.11.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
